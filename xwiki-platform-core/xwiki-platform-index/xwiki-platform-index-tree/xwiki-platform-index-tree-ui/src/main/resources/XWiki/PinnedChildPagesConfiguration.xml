#set ($configObject = $doc.getObject('XWiki.PinnedChildPagesClass', true))
#set ($configClass = $configObject.xWikiClass)
#set ($discard = $doc.use($configObject))
&lt;form action="$escapetool.xml($doc.getURL('save'))" method="post" class="xform"&gt;
  &lt;fieldset&gt;
    &lt;input type="hidden" name="form_token" value="$!services.csrf.token" /&gt;
    ## Add the configuration object if it doesn't exist.
    &lt;input type="hidden" name="objectPolicy" value="updateOrCreate" /&gt;
    &lt;input type="hidden" name="comment" value="$escapetool.xml($services.localization.render(
      'index.tree.pinnedChildPages.saveComment'))" /&gt;
    &lt;dl&gt;
      #set ($configPropertyId = 'pinnedChildPages')
      #set ($configProperty = $configClass.get($configPropertyId))
      &lt;dt&gt;
        &lt;label for="$escapetool.xml($configPropertyId)"&gt;
          $escapetool.xml($configProperty.translatedPrettyName)
        &lt;/label&gt;
        &lt;span class="xHint"&gt;$!escapetool.xml($configProperty.hint)&lt;/span&gt;
      &lt;/dt&gt;
      &lt;dd&gt;
        #unwrapXPropertyDisplay($doc.display($configProperty.name, 'edit'))
      &lt;/dd&gt;
    &lt;/dl&gt;
  &lt;/fieldset&gt;
  &lt;div class="bottombuttons"&gt;
    &lt;p class="admin-buttons"&gt;
      &lt;span class="buttonwrapper"&gt;
        &lt;input class="button" type="submit" name="action_saveandcontinue"
          value="$escapetool.xml($services.localization.render('admin.save'))" /&gt;
      &lt;/span&gt;
    &lt;/p&gt;
  &lt;/div&gt;
&lt;/form&gt;
{{/html}}</codeToExecute>
